  <h2>Nearest Stations:</h2>
  <% @station_search.stations.each do |station| %>
<ul class="station">
    <li class="name"><%= station.name %></li>
    <li class="address"><%= station.address %></li>
    <li class="fuel-type"><%= station.fuel_types %></li>
    <li class="distance"><%= station.distance %></li>
    <li class="access-time"><%= station.access_times %></li>
  <% end %>
  </ul>
  
